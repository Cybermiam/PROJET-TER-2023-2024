@prefix :  <http://patrimalp.org/brocarts/thesaurus#> .
@prefix brocartsgraph:  <http://patrimalp.org/brocarts/graph/> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

:Theme  rdf:type         owl:Class ;
        rdfs:subClassOf  skos:Concept ;
        rdfs:label       "Thème" ;
        rdfs:comment     "Définit le thème d'une œuvre" .

:theme1  rdf:type        :Theme ;
        skos:definition  "Définit le thème d'une œuvre" ;
        skos:prefLabel   "Type Theme" .

:theme2  rdf:type       :Theme ;
        skos:broader    :theme1 ;
        skos:prefLabel  "Géométrique" .

:theme3  rdf:type       :Theme ;
        skos:broader    :theme1 ;
        skos:prefLabel  "Végétal" .

:theme4  rdf:type       :Theme ;
        skos:broader    :theme1 ;
        skos:prefLabel  "Ornementaux" .
