        skos:broader    :typeOfBrocade1 ;
        skos:prefLabel  "Juxtaposé" .

:typeOfBrocade4  rdf:type  :TypeOfBrocade ;
        skos:broader    :typeOfBrocade1 ;
        skos:prefLabel  "Parsemé" .

:typeOfBrocade5  rdf:type  :TypeOfBrocade ;
